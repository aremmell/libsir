#include "chsir.h"

uint32_t
sir_getversionhex(void) {
    void *fptr;
    uint32_t retval;

    fptr = dlsym(_Chsir_handle, "sir_getversionhex_chdl");
    if (fptr == NULL) {
        fprintf(_stderr, "Error: %s(%d): dlsym(): %s\n",
                __func__, __LINE__, dlerror());
        return 0;
    }
    dlrunfun(fptr, &retval, sir_getversionhex);

    return retval;
}
