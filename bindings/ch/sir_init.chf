#include "chsir.h"

bool
sir_init(sirinit* si) {
    void* fptr;
    bool retval;

    fptr = dlsym(_Chsir_handle, "sir_init_chdl");
    if (fptr == NULL) {
        fprintf(_stderr, "Error: %s(%d): dlsym(): %s\n",
                __func__, __LINE__, dlerror());
        return false;
    }
    dlrunfun(fptr, &retval, sir_init, si);

    return retval;
}
